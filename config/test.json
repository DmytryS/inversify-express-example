{
    "DB": {
        "type": "mongo",
        "url": "mongodb://localhost:27017/inversify-boilerplate-test"
    },
    "SERVER": {
        "port": 3000,
        "baseUrl": "/api/v1",
        "uiUrl": "http://localhost"
    },
    "SWAGGER": {
        "enabled": false
    },
    "LOGGER": {
        "path": "/logs",
        "fileName": "inversify-example.log"
    },
    "AUTH": {
        "secret": "SOME_SECRET",
        "expiresIn": "10d",
        "saltRounds": 3
    },
    "MAIL": {
        "from": "user@gmail.com",
        "transport_options": {
            "host": "smpt.gmail.com",
            "service": "Gmail",
            "auth": {
                "user": "user@gmail.com",
                "pass": "password"
            }
        }
    }
}
